{
  "name": "suiseki",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "format:style": "stylelint **/*.svelte **/*.scss",
    "fix:style": "stylelint **/*.svelte **/*.scss --fix",
    "cypress": "cypress run",
    "e2e": "npm-run-all format --parallel --race dev cypress"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@sentry/cli": "^1.73.2",
    "@tsconfig/svelte": "^3.0.0",
    "cypress": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.9",
    "sirv-cli": "^2.0.2",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-i18n": "^3.3.13",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@sentry/browser": "^6.18.2",
    "aplayer": "^1.10.1",
    "dayjs": "^1.10.8"
  },
  "license": "MIT"
}
