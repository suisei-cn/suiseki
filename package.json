{
  "name": "suiseki",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "format:style": "stylelint **/*.svelte **/*.scss",
    "fix:style": "stylelint **/*.svelte **/*.scss --fix",
    "cypress": "cypress run",
    "e2e": "npm-run-all format --parallel --race dev cypress"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@sentry/cli": "^1.62.0",
    "@tsconfig/svelte": "^1.0.10",
    "cypress": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "^2.1.6",
    "rollup": "^2.39.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "sirv-cli": "^1.0.11",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.36",
    "svelte-i18n": "^3.3.5",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@sentry/browser": "^6.2.0",
    "aplayer": "^1.10.1",
    "dayjs": "^1.10.4"
  },
  "license": "MIT"
}
